{
  "name": "bitbucket-chat-bot",
  "version": "2.0.0",
  "description": "A simple Hangouts Chat Bot to show information about BitBucket",
  "main": "bot.js",
  "scripts": {
    "start": "DEBUG=botkit:* node bot.js",
    "dev": "nodemon bot.js",
    "test": "node_modules/.bin/mocha",
    "mocha": "mocha spec"
  },
  "author": "Julien Bras <julien.bras@gbandsmith.com>",
  "license": "MIT",
  "repository": "github:gbandsmith/bitbucket-chat-bot",
  "bugs": "https://github.com/gbandsmith/bitbucket-chat-bot/issues",
  "homepage": "https://github.com/gbandsmith/bitbucket-chat-bot#readme",
  "keywords": [
    "bots",
    "hangouts chat",
    "bitbucket"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "botkit": "github:bobman38/botkit#google_cred_as_env",
    "botkit-storage-mongo": "^1.0.7",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "node-env-file": "^0.1.8"
  },
  "devDependencies": {
    "botium-bindings": "^2.0.1",
    "mocha": "^5.2.0",
    "botium-connector-echo": "latest"
